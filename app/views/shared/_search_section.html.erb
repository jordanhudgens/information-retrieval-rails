<header>
  <%= link_to "All Questions", questions_path %>

  <%= form_tag questions_path, method: :get do %>
    <%= text_field_tag :q, nil, placeholder: "Search", class: 'form-control input-lg' %>
  <% end %>

  <% if @questions.suggestions.any? %>
    <p>Did you mean: <%= @questions.suggestions.first %></p>
  <% end %>
</header>